{
  "root": {
    "path": "",
    "on_null": "return_empty"
  },
  "pipeline": [
    {
      "op": "copy",
      "from": "Description",
      "to": "reduced_results.description"
    },
    {
      "op": "copy",
      "from": "Commands[].Command",
      "to": "reduced_results.commands_command"
    },
    {
      "op": "copy",
      "from": "Commands[].Usecase",
      "to": "reduced_results.commands_usecase"
    },
    {
      "op": "prune",
      "strategy": "empty_values",
      "target": "reduced_results"
    }
  ],
  "output": {
    "result": "$working.reduced_results"
  }
}
